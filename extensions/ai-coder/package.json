{
  "name": "ai-training",
  "displayName": "AITraining",
  "description": "A chat interface extension for VSCode with React UI",
  "version": "0.0.7-beta.1",
  "publisher": "s3-ai",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": ["Other"],
  "main": "./out/extension",
  "activationEvents": ["*"],
  "contributes": {
    "keybindings": [
      {
        "command": "extension.selectToChat",
        "key": "cmd+l",
        "when": "editorTextFocus"
      }
    ],
    "submenus": [
      {
        "id": "ai-training.submenu",
        "label": "AI Training"
      }
    ],
    "commands": [
      {
        "command": "extension.addToChat",
        "title": "Add to Chat"
      },
      {
        "command": "extension.selectToChat",
        "title": "Send Selection to Chat"
      },
      {
        "command": "ai-training.newChat",
        "title": "New Chat",
        "icon": "$(add)"
      },
      {
        "command": "ai-training.showHistory",
        "title": "History",
        "icon": "$(history)"
      },
      {
        "command": "ai-training.addToChat",
        "title": "Add Selection to Chat"
      },
      {
        "command": "ai-training.logOut",
        "title": "Log Out"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "ai-training.submenu",
          "when": "editorTextFocus"
        }
      ],
      "ai-training.submenu": [
        {
          "command": "ai-training.addToChat"
        },
        {
          "command": "ai-training.logOut"
        }
      ],
      "view/title": [
        {
          "command": "ai-training.newChat",
          "when": "view == chatView",
          "group": "navigation"
        },
        {
          "command": "ai-training.showHistory",
          "when": "view == chatView",
          "group": "navigation"
        },
        {
          "command": "ai-training.logOut",
          "when": "view == chatView",
          "group": "2_logout"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "chat-sidebar",
          "title": "AITraining",
          "icon": "resources/chat.svg"
        }
      ]
    },
    "views": {
      "chat-sidebar": [
        {
          "type": "webview",
          "id": "chatView",
          "name": "Chat"
        }
      ]
    },
    "configuration": {
      "title": "AI Training",
      "properties": {
        "ai-training.telemetry.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable telemetry data collection for AI Training extension"
        },
        "ai-training.telemetry.batchSize": {
          "type": "number",
          "default": 10,
          "description": "Number of events to batch before sending"
        },
        "ai-training.telemetry.batchInterval": {
          "type": "number",
          "default": 30000,
          "description": "Interval in milliseconds to send batched events"
        },
        "ai-training.telemetry.debugMode": {
          "type": "boolean",
          "default": false,
          "description": "Enable debug mode for telemetry (logs events without sending)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && webpack --mode production",
    "watch": "tsc -watch -p ./ & webpack --mode development --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "rm -rf out dist",
    "build": "npm run clean && npm run compile",
    "rebuild": "npm run clean && npm run compile && code --disable-extension ai-training && code",
    "test": "vscode-test",
    "build-package": "vsce package",
    "fix-all": "npm run format && npm run lint:fix"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@tailwindcss/postcss": "^4.1.5",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.11",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/vscode": "^1.99.1",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@stylistic/eslint-plugin-ts": "^2.12.1",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-jsdoc": "^50.3.1",
    "@vscode/test-cli": "^0.0.10",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.26.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.17",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.3",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-color-chips": "^0.1.3",
    "remark-gfm": "^4.0.1",
    "remixicon": "^4.6.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "3.3",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@remixicon/react": "^4.6.0",
    "@types/lodash": "^4.17.16",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^10.0.0",
    "@vscode/test-electron": "^2.5.2",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dexie": "^4.0.11",
    "express": "^5.1.0",
    "gtag": "^1.0.1",
    "highlight.js": "^11.11.1",
    "lodash": "^4.17.21",
    "minimatch": "^10.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-window": "^1.8.11",
    "uuid": "^11.1.0"
  }
}
